<?xml version="1.0" encoding="UTF-8"?>
<AssignmentRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <assignmentRule>
        <fullName>Lead Queue Assignment</fullName>
        <active>true</active>
        <ruleEntry>
            <assignedTo>Lead_Research_Queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.LeadSource</field>
                <operation>equals</operation>
                <value>Inbound</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Timestamp_SSSU_lead__c</field>
                <operation>equals</operation>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>Outbound_Leads_Queue</assignedTo>
            <assignedToType>Queue</assignedToType>
            <criteriaItems>
                <field>Lead.LeadSource</field>
                <operation>equals</operation>
                <value>Outbound</value>
            </criteriaItems>
            <criteriaItems>
                <field>Lead.Timestamp_SSSU_lead__c</field>
                <operation>equals</operation>
            </criteriaItems>
        </ruleEntry>
        <ruleEntry>
            <assignedTo>salesexcellence-sssu@heyjobs.de</assignedTo>
            <assignedToType>User</assignedToType>
            <criteriaItems>
                <field>Lead.Timestamp_SSSU_lead__c</field>
                <operation>notEqual</operation>
            </criteriaItems>
        </ruleEntry>
    </assignmentRule>
</AssignmentRules>
