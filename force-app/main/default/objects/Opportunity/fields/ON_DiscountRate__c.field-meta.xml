<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ON_DiscountRate__c</fullName>
    <description>Helper Field: shows the discount in percent that is applied</description>
    <formula>IF(
    OR(
        SkontoAktiv__c,
        Account.SkontoAktiv__c
    ),
    0.02,
    NULL
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Helper Field: shows the discount in percent that is applied</inlineHelpText>
    <label>JustOn: Discount Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Percent</type>
</CustomField>
