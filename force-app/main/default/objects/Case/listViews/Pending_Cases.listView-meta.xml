<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Pending_Cases</fullName>
    <columns>OWNER_NAME</columns>
    <columns>CASES.RECORDTYPE</columns>
    <columns>ACCOUNT.NAME</columns>
    <columns>X1st_Job_Creation_Case__c</columns>
    <columns>Segment_V02__c</columns>
    <columns>CASES.STATUS</columns>
    <columns>Account_Owner__c</columns>
    <columns>CASES.CASE_NUMBER</columns>
    <columns>bHasBeenPublishedBefore__c</columns>
    <columns>CASES.CREATED_DATE_DATE_ONLY</columns>
    <columns>txtJobId__c</columns>
    <filterScope>Everything</filterScope>
    <filters>
        <field>CASES.STATUS</field>
        <operation>notEqual</operation>
        <value>New,Escalated,Closed,Job Submitted,Done,Go-Live Scheduled,Job Live,Lost</value>
    </filters>
    <filters>
        <field>CASES.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Case.JobCreationCase</value>
    </filters>
    <filters>
        <field>CORE.USERS.LAST_NAME</field>
        <operation>notEqual</operation>
        <value>reinik</value>
    </filters>
    <filters>
        <field>Created_Date__c</field>
        <operation>lessThan</operation>
        <value>LAST_N_DAYS:14</value>
    </filters>
    <label>Job Creation Cases older than 2 weeks</label>
</ListView>
