<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Minutes_to_SLA__c</fullName>
    <description>Shows minutes before SLA violation</description>
    <externalId>false</externalId>
    <formula>IF( 
OR( ISBLANK( tysosla__First_Touch_Due__c ), NOT( ISBLANK(  tysosla__First_Touch__c )) ), null,
(tysosla__First_Touch_Due__c - NOW())*24*60
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Shows minutes before SLA violation</inlineHelpText>
    <label>Minutes to SLA</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
