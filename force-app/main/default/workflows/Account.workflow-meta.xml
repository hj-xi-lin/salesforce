    <rules>
        <fullName>Task for framework contract negotiations</fullName>
        <active>false</active>
        <criteriaItems>
            <field>Account.Framework_Contract_Amount__c</field>
            <operation>greaterThan</operation>
            <value>EUR 0</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Record_Type__c</field>
            <operation>notEqual</operation>
            <value>Agency Customer Sales</value>
        </criteriaItems>
        <description>4 months before the end of an framework contract, the AM receives a task to remind them to start negotiations to renew contract</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Start_Framework_Contract_negotiations</name>
                <type>Task</type>
            </actions>
            <offsetFromField>Account.Framework_Contract_Signed_Date__c</offsetFromField>
            <timeLength>270</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
