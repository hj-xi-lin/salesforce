    <rules>
        <fullName>Follow up Task for AMs</fullName>
        <actions>
            <name>Follow_Up</name>
            <type>Task</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>Opportunity.OwnerProfile__c</field>
            <operation>equals</operation>
            <value>SMB Account Manager,Account Manager,Head of Account Management,Head of SMB Account Management,Head of LCS Account Management,LCS Account Manager,Enterprise Account Manager</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Follow_Up_Date__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Record_Type__c</field>
            <operation>notEqual</operation>
            <value>Agency Customer Business</value>
        </criteriaItems>
        <description>Creates an immediate task for AMs the moment they fill in the &apos;Follow up date&apos; on the opportunity.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>HeyPacket_OpportunityClosedWon</fullName>
        <actions>
            <name>HeyPaket_OpportunityClosedWon_updated</name>
            <type>OutboundMessage</type>
        </actions>
        <active>false</active>
        <booleanFilter>(1 AND 3 AND 4 AND 5 AND 7 ) OR (2 AND 6 AND 7)</booleanFilter>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Contract received</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.StageName</field>
            <operation>equals</operation>
            <value>Closed Won</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Test_Account__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Record_Type__c</field>
            <operation>notEqual</operation>
            <value>Inventory Business</value>
        </criteriaItems>
        <criteriaItems>
            <field>Account.Name</field>
            <operation>notContain</operation>
            <value>Test</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.Instant_Purchase__c</field>
            <operation>equals</operation>
            <value>True</value>
        </criteriaItems>
        <criteriaItems>
            <field>Opportunity.HeyPaket_Type__c</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <description>Triggers Outbound Message to send Data to Slack via Zapier</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
