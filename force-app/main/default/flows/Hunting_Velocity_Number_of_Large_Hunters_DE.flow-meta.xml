    <formulas>
        <name>fmlFirstDayOfThisWeek</name>
        <dataType>Date</dataType>
        <expression>CASE(WEEKDAY(TODAY()), 
1,TODAY() - 6 , 
2, TODAY(),
3, TODAY() - 1,
4, TODAY() - 2,
5, TODAY() - 3,
6, TODAY() - 4,
TODAY() - 5
)</expression>
    </formulas>
    <formulas>
        <name>fmlLastDayOfThisWeek</name>
        <dataType>Date</dataType>
        <expression>CASE(WEEKDAY(TODAY()), 
1,TODAY(), 
2, TODAY() + 6,
3, TODAY() + 5,
4, TODAY() + 4,
5, TODAY() + 3,
6, TODAY() + 2,
TODAY() +1
)</expression>
    </formulas>
