        <rules>
            <name>Yes</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Reach_Campaign_Start_Date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
