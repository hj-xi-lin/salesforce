    <decisions>
        <name>Check_if_Reach_Campaign_date_is_present</name>
        <label>Check if Reach Campaign date is present</label>
        <locationX>446</locationX>
        <locationY>276</locationY>
        <defaultConnector>
            <targetReference>Check_for_annotations</targetReference>
        </defaultConnector>
