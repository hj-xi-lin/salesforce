        <defaultConnectorLabel>All Other Recrod Types</defaultConnectorLabel>
        <rules>
            <name>Sales_Agency_Acquisition</name>
            <conditionLogic>(1 OR 2 OR 4) AND 3</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Opportunity.RecordType.DeveloperName</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Agency_Acquisition</stringValue>
