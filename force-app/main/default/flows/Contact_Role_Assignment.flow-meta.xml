    <formulas>
        <name>fmlDealfrontInfluence</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!varExistingValue}),
&quot;Dealfront;&quot;,
IF(INCLUDES({!$Record.Opportunity.Sales_Signal__c}, &quot;Dealfront&quot;),{!varExistingValue},
{!varExistingValue}&amp;&quot;;&quot;&amp;&quot;Dealfront&quot; 
)
)</expression>
    </formulas>
    <formulas>
        <name>fmlMqlType</name>
        <dataType>Number</dataType>
        <expression>CASE({!$Record.Contact.MQL_Type_Contact__c}, 
&quot;Type 3&quot;, 3, 
&quot;Type 2&quot;, 2,
&quot;Type 1&quot;,1,
4)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <name>fmltodayminus365</name>
        <dataType>Date</dataType>
        <expression>TODAY() - 365</expression>
    </formulas>
