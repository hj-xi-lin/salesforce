                <rightValue>
                    <stringValue>Budget boost</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Create_Repeat_Purchase_case_3</targetReference>
            </connector>
            <label>Budget Boost</label>
        </rules>
