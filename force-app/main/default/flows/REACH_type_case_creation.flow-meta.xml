    <decisions>
        <name>Check_Deal_Type</name>
        <label>Check Deal Type</label>
        <locationX>974</locationX>
        <locationY>134</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>New</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varRecord.REACH_Deal_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>New</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Get_Open_ATS_Onboarding_cases</targetReference>
            </connector>
            <label>New</label>
        </rules>
        <rules>
            <name>Renewal</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varRecord.REACH_Deal_Type__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>Renewal</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>CreateRepeatPurchasecase</targetReference>
            </connector>
            <label>Renewal</label>
        </rules>
        <rules>
            <name>Renewal_with_campaign_adjustment_added_campaign</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varRecord.REACH_Deal_Type__c</leftValueReference>
                <operator>EqualTo</operator>
