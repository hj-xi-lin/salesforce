    <decisions>
        <name>Active_Heypakete_Start_Dates</name>
        <label>Active Heypakete Start Dates</label>
        <locationX>1106</locationX>
        <locationY>242</locationY>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>New_Start_Date_Active_End_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varAccount.Active_HeyPaket__r.HeyPaket_Status__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <stringValue>Cancelled</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varQuote.HeyPaket_Start_Date__c</leftValueReference>
                <operator>GreaterThan</operator>
                <rightValue>
                    <elementReference>varAccount.Active_HeyPaket__r.HeyPaket_End_Date__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_start_date_end_date_error</targetReference>
            </connector>
            <label>New_Start_Date &gt; Active_End_Date</label>
        </rules>
        <rules>
            <name>New_Start_Date_Active_End_Date_2</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varQuote.HeyPaket_Start_Date__c</leftValueReference>
                <operator>LessThan</operator>
                <rightValue>
                    <elementReference>varAccount.Active_HeyPaket__r.HeyPaket_End_Date__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>start_date_end_date_error</targetReference>
            </connector>
            <label>New Start Date &lt; Active End Date</label>
        </rules>
    </decisions>
    <decisions>
        <name>Start_Date_Validation</name>
        <label>Start Date Validation</label>
        <locationX>842</locationX>
        <locationY>134</locationY>
        <defaultConnectorLabel>Default</defaultConnectorLabel>
        <rules>
            <name>No_Start_Date</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varQuote.HeyPaket_Start_Date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_no_start_date_error</targetReference>
            </connector>
            <label>No Start Date</label>
        </rules>
        <rules>
            <name>Start_Date_in_the_past</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varQuote.HeyPaket_Start_Date__c</leftValueReference>
                <operator>LessThanOrEqualTo</operator>
                <rightValue>
                    <elementReference>fmlYesterday</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_past_start_date_error</targetReference>
            </connector>
            <label>Start Date in the past</label>
        </rules>
        <rules>
            <name>Start_Date_Next_HeyPaket_available</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varQuote.HeyPaket_Start_Date__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>varAccount.Next_HeyPaket__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Assign_next_active_packet_error</targetReference>
            </connector>
            <label>Start Date + Next HeyPaket available</label>
        </rules>
        <rules>
            <name>Active_HeyPaket_available</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>varAccount.Active_HeyPaket__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Active_Heypakete_Start_Dates</targetReference>
            </connector>
            <label>Active HeyPaket available</label>
        </rules>
    </decisions>
